package com.project.demo.enumeration;

public enum Role {
	// ===== 前台 =====
    GUEST,       // 訪客，未登入，僅能瀏覽商品與資訊，無法購物或查看個人資料
    CUSTOMER,    // 顧客，註冊用戶，可購物、下單、管理自己的訂單與個人資料
    SELLER,      // 賣家，認證用戶，可管理商品、訂單、庫存
    
    // ===== 後台 =====
    USER,        // 一般使用者
    ADMIN,       // 系統管理員
    SUPER_ADMIN, // 超級管理員
}
